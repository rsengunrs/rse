{
	"extends": "@cloudflare/workers-tsconfig/tsconfig.json",
	"compilerOptions": {
		"module": "esnext",
		"types": ["node"],
		"jsx": "react"
	},
	"include": ["**/*.ts", "**/*.tsx", "**/*.js"],
	"exclude": [
		"emitted-types",
		"node_modules",
		"*-dist",
		"src/miniflare-cli",
		"templates",
		"**/__tests__",
		"**/*.test.ts"
	]
}
