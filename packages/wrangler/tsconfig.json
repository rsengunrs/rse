{
	"extends": "@cloudflare/workers-tsconfig/tsconfig.json",
	"compilerOptions": {
		"module": "CommonJS",
		"types": ["node"],
		"jsx": "react"
	},
	"include": [
		"**/*.ts",
		"**/*.tsx",
		"**/*.js",
		"../pages-shared/project/hash.ts"
	],
	"exclude": [
		"emitted-types",
		"node_modules",
		"*-dist",
		"src/miniflare-cli",
		"templates",
		"**/__tests__",
		"**/*.test.ts"
	]
}
